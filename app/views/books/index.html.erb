 <body>
   <p id="notice"></p>
   <h1>Books</h1>
   <table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
        <% @books.each do |book| %>
        <tr>
         <td><%= book.title %></td>
         <td><%= book.body %></td>
        </tr>
        <% end %>
  </tbody>
</table>



<% if @book.errors.any? %>
  <%= @book.errors.count %>
  <% @book.errors.full_messages.each do |message| %>
    <%= message %>
  <% end %>
<% end %>

<h2>New book</h2>

<%= form_with model: @book, url:'/books', local:true do |f| %>
  <h4>Title</h4>
    <%= f.text_field :title %>

  <h4>Body</h4>
    <%= f.text_area :body %>
    <%= f.submit 'Create Book' %>
  <% end %>

</body>
